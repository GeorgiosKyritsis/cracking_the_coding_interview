package chapter2;

public class Tester {

	public static void main(String[] args) {
		SinglyLinkedList ls = new SinglyLinkedList();
		ls.add(1);
		ls.add(2);
		ls.add(30);
		ls.add(111);
		ls.remove();
		ls.add(40);
		ls.printList();
		
		System.out.println(ls.size);
		
		System.out.println();
		
		DoublyLinkedList ls1 = new DoublyLinkedList();
		ls1.add(1);
		ls1.add(2);
		ls1.add(30);
		ls1.printList();
		
		System.out.println(ls1.contains(30));
		
		System.out.println();

		DoublyLinkedList2 ls2 = new DoublyLinkedList2();
		ls2.addFirst(1);
		ls2.addFirst(2);
		ls2.addFirst(10);
		ls2.addLast(10);
		ls2.removeFirst();
		ls2.removeFirst();
		ls2.printList();

	}

}
