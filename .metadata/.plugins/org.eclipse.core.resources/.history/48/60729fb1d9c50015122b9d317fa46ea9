		SinglyLinkedList ls = new SinglyLinkedList();
		ls.add(10);
		ls.add(20);
		ls.add(30);
		ls.add(40);
		ls.add(50);
		ls.add(30);
		ls.add(10);
		ls.add(10);
		ls.add(50);
		ls.add(10);
		ls.add(10);
		ls.add(10);
		ls.add(10);
		ls.printList();
		System.out.println();
		SinglyLinkedList ls2 = ls.partition(30);
		ls2.printList();
		System.out.println();
		
		ls.deleteMiddleNode();
		ls.printList();
		System.out.println();
		ls.removeDuplicates();
		ls.printList();
		System.out.println();
		ls.printKthFromLastElement(2);
		System.out.println();
		ls.deleteMiddleNode();
		ls.printList();
		
		 
		
		
	}

