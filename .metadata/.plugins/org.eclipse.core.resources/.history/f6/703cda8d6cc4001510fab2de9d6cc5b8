package chapter1;

public class stringCompression {
	
	public static String compress(String str)
	{
		char[] c = str.toCharArray();
		int counter = 0;
		String ans;
		int index = 0;
		
		for (int i = 0; i < c.length-1; i++)
		{
			if (c[i] == c[i+1])
			{
				counter++;
				index = i;;
			}
		}
	}

}
